{
	"name": "ChangeColor",
	"manifest_version": 2,
	"version": "0.1",
	"description":"Chrome Extension to change Background Color of any Website with just one click.",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Change Color",
		"default_popup": "popup.html"
	},
	"author": "Er Vaibhav Karnwal"
}